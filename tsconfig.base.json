{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "esnext",
    "baseUrl": ".",
    "sourceMap": false,
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "removeComments": false,
    "rootDir": ".",
    "types": [],
    "paths": {
      "@element-ai/components": ["packages/components"],
      "@element-ai/components/*": ["packages/components/*"],
      "@element-ai/utils": ["packages/utils"],
      "@element-ai/utils/*": ["packages/utils/*"],
      "@element-ai/hooks": ["packages/hooks"],
      "@element-ai/directives": ["packages/directives"],
      "@element-ai/constants": ["packages/constants"],
      "@element-ai/locale": ["packages/locale"],
      "@element-ai/locale/*": ["packages/locale/*"],
      "element-ai": ["packages/element-ai"]
    }
  }
}
